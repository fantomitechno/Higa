{
  "name": "higa",
  "version": "0.0.4",
  "description": "A package to interract with the Discord API coded by myself with randomness",
  "main": "build",
  "typings": "types",
  "scripts": {
    "build": "tsc",
    "dev:build": "tsc -w",
    "prettier": "prettier **.json src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "generate:changelog": "auto-changelog",
    "generate:doc": "typedoc --options typedoc.json"
  },
  "keywords": [
    "nodejs",
    "discord",
    "discord-bot",
    "discord-api"
  ],
  "dependencies": {
    "axios": "^0.26.0",
    "discord-api-types": "^0.27.3",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "auto-changelog": "^2.4.0",
    "typedoc": "^0.22.12",
		"typedoc-darker-theme": "^1.2.1"
  },
  "auto-changelog": {
    "output": "HISTORY.md",
    "template": "changelog-template.hbs",
    "package": true,
    "commitLimit": false
  },
  "author": "fantomitechno",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  }
}
