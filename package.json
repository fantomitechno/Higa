{
  "name": "higa",
  "version": "0.0.3",
  "description": "",
  "main": "build",
  "typings": "types",
  "scripts": {
    "build": "tsc",
    "dev:build": "tsc -w",
    "prettier": "prettier **.json src --write",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "generate:changelog": "auto-changelog",
    "generate:doc": "typedoc --options typedoc.json"
  },
  "keywords": [
    "nodejs",
    "discord",
    "discord-bot",
    "discord-api"
  ],
  "dependencies": {
    "node-fetch": "2.6.6",
    "discord-api-types": "^0.26.1",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "auto-changelog": "^2.3.0",
    "typedoc": "^0.21.9",
		"typedoc-darker-theme": "^1.2.1"
  },
  "auto-changelog": {
    "output": "HISTORY.md",
    "template": "changelog-template.hbs",
    "package": true,
    "commitLimit": false
  },
  "author": "fantomitechno",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  }
}
